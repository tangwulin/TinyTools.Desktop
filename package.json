{
  "name": "tinytools.desktop",
  "version": "3.99.1-beta.6",
  "description": "An Electron application with Vue",
  "main": "./out/main/index.js",
  "author": "tangwulin",
  "homepage": "https://github.com/tangwulin/TinyTools.Desktop/",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win_x64": "pnpm run build && electron-builder --win --config --x64",
    "build:win_arm64": "pnpm run build && electron-builder --win --config --arm64",
    "build:win_ia32": "pnpm run build && electron-builder --win --config --ia32",
    "build:mac": "pnpm run build && electron-builder --mac --config",
    "build:linux": "pnpm run build && electron-builder --linux --config",
    "release": "node scripts/release.mjs"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": " ^2.0.0",
    "@vicons/antd": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "electron-updater": "^6.1.1",
    "lodash-es": "^4.17.21",
    "modern-screenshot": "^4.4.29",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue-router": "4",
    "vuedraggable-swap": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.15",
    "electron": "22.3.23",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.1.3",
    "naive-ui": "^2.34.4",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vue": "^3.3.4"
  }
}
{
  "name": "tinytools-desktop",
  "version": "4.4.2",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "tangwulin",
  "homepage": "https://github.com/tangwulin/TinyTools.Desktop",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win_x64": "npm run build && npm run removeSourceMap && electron-builder --win --config --x64",
    "build:win_arm64": "npm run build && npm run removeSourceMap && electron-builder --win --config --arm64",
    "build:win_ia32": "npm run build && npm run removeSourceMap && electron-builder --win --config --ia32",
    "build:win": "npm run build && npm run removeSourceMap && electron-builder --win --config --x64 --ia32",
    "build:mac": "npm run build && npm run removeSourceMap && electron-builder --mac --config",
    "build:linux": "npm run build && npm run removeSourceMap && electron-builder --linux --config",
    "removeSourceMap": "node ./scripts/removeSourceMap.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@sentry/electron": "^4.21.0",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "extract-file-icon-fix": "^0.3.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lucky-canvas/vue": "^0.1.11",
    "@rushstack/eslint-patch": "^1.10.1",
    "@sentry/vite-plugin": "^2.16.1",
    "@sentry/vue": "^7.109.0",
    "@tomjs/electron-devtools-installer": "^2.2.1",
    "@types/howler": "^2.2.11",
    "@types/node": "^20.12.3",
    "@types/parallax-js": "^3.1.3",
    "@types/progress": "^2.0.7",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/rxjs": "^10.9.0",
    "alova": "^2.18.2",
    "autoprefixer": "^10.4.19",
    "deepcopy": "^2.1.0",
    "dexie": "^3.2.7",
    "echarts": "^5.5.0",
    "electron": "^22.3.27",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "howler": "^2.2.4",
    "js-event-bus": "^1.1.1",
    "lodash-es": "^4.17.21",
    "modern-screenshot": "^4.4.38",
    "naive-ui": "^2.38.1",
    "nodejs-file-downloader": "^4.12.1",
    "parallax-js": "^3.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "rxjs": "^7.8.1",
    "sass": "npm:sass-embedded@^1.74.1",
    "swiper": "^11.1.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "5.2.8",
    "vite-plugin-inspect": "^0.8.3",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.7",
    "vuedraggable-swap": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  }
}

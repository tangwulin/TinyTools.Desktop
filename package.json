{
  "name": "tinytools-desktop",
  "version": "4.6.2",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "tangwulin",
  "homepage": "https://github.com/tangwulin/TinyTools.Desktop",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "npm electron-builder install-app-deps",
    "build:win_x64": "npm run build && npm run removeSourceMap && pnpm electron-builder --win --config --x64",
    "build:win_arm64": "npm run build && npm run removeSourceMap && pnpm electron-builder --win --config --arm64",
    "build:win_ia32": "npm run build && npm run removeSourceMap && pnpm electron-builder --win --config --ia32",
    "build:win": "npm run build && npm run removeSourceMap && pnpm electron-builder --win --config --x64 --ia32",
    "build:mac": "npm run build && npm run removeSourceMap && pnpm electron-builder --mac --config",
    "build:linux": "npm run build && npm run removeSourceMap && pnpm electron-builder --linux --config",
    "removeSourceMap": "node ./scripts/removeSourceMap.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@sentry/electron": "^4.24.0",
    "electron-log": "^5.2.0",
    "electron-updater": "^6.3.4",
    "node-geticon": "file:src\\main\\lib\\node-geticon"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lucky-canvas/vue": "^0.1.11",
    "@rushstack/eslint-patch": "^1.10.4",
    "@sentry/vite-plugin": "^2.22.4",
    "@sentry/vue": "^8.28.0",
    "@tomjs/electron-devtools-installer": "^2.3.2",
    "@types/howler": "^2.2.11",
    "@types/node": "^16.18.108",
    "@types/parallax-js": "^3.1.3",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^11.0.3",
    "@vueuse/rxjs": "^11.0.3",
    "alova": "^3.0.16",
    "autoprefixer": "^10.4.20",
    "deepcopy": "^2.1.0",
    "dexie": "^3.2.7",
    "echarts": "^5.5.1",
    "electron": "^22.3.27",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "howler": "^2.2.4",
    "js-event-bus": "^1.1.1",
    "lodash-es": "^4.17.21",
    "modern-screenshot": "^4.4.39",
    "naive-ui": "^2.39.0",
    "nodejs-file-downloader": "^4.13.0",
    "parallax-js": "^3.1.0",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.0",
    "pinyin-match": "^1.2.6",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "rxjs": "^7.8.1",
    "sass": "npm:sass-embedded@^1.78.0",
    "swiper": "^11.1.12",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "5.2.8",
    "vite-plugin-inspect": "^0.8.7",
    "vue": "^3.5.3",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.6",
    "vuedraggable-swap": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  }
}

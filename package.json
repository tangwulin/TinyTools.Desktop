{
  "name": "tinytools-desktop",
  "version": "4.0.1",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "tangwulin",
  "homepage": "https://github.com/tangwulin/TinyTools.Desktop",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win_x64": "npm run build && electron-builder --win --config --x64",
    "build:win_arm64": "npm run build && electron-builder --win --config --arm64",
    "build:win_ia32": "npm run build && electron-builder --win --config --ia32",
    "build:win": "npm run build && electron-builder --win --config --x64 && electron-builder --win --config --ia32",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "updateAvatars": "ts-node ./scripts/updateBuiltInAvatar.ts --files"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@sentry/electron": "^4.17.0",
    "@sentry/vite-plugin": "^2.10.2",
    "@vueuse/core": "^10.7.0",
    "@vueuse/rxjs": "^10.7.0",
    "axios": "^1.6.2",
    "deepcopy": "^2.1.0",
    "dexie": "^3.2.4",
    "echarts": "^5.4.3",
    "electron-updater": "^6.1.7",
    "extract-file-icon-fix": "^0.3.3",
    "howler": "^2.2.4",
    "lodash-es": "^4.17.21",
    "lucky-canvas": "^1.7.27",
    "parallax-js": "^3.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "rxjs": "^7.8.1",
    "swiper": "^11.0.5",
    "vue-echarts": "^6.6.2",
    "vue-router": "4",
    "vuedraggable-swap": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/howler": "^2.2.11",
    "@types/node": "^20.10.3",
    "@types/parallax-js": "^3.1.3",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "electron": "^22.3.27",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "modern-screenshot": "^4.4.36",
    "naive-ui": "^2.35.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "puppeteer": "^21.6.0",
    "sass": "^1.70.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "5.0.12",
    "vue": "^3.3.10",
    "vue-tsc": "^1.8.25"
  }
}
